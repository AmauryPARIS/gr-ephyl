<?xml version="1.0"?>
<block>
  <name>Concatenate
Messages</name>
  <key>ephyl_msg_mux</key>
  <category>[ephyl]</category>
  <import>import ephyl</import>
  <make>ephyl.msg_mux($list_sensor, $log)</make>

  <param>
    <name>List of sensor ID</name>
    <key>list_sensor</key>
    <value>[0]</value>
    <type>raw</type>
  </param>

  <param>
    <name>Generate debug/log file</name>
    <key>log</key>
    <value>False</value>
    <type>bool</type>
  </param>

  <sink>
    <name>frame_n</name>
    <type>message</type>
  </sink>

  <sink>
    <name>slot_n</name>
    <type>message</type>
  </sink>

  <sink>
    <name>data</name>
    <type>message</type>
  </sink>

  <sink>
    <name>ULCCH</name>
    <type>message</type>
  </sink>

  <sink>
    <name>detect</name>
    <type>message</type>
  </sink>

  <source>
    <name>final_msg</name>
    <type>message</type>
  </source>

  <source>
    <name>feedback</name>
    <type>message</type>
  </source>


</block>