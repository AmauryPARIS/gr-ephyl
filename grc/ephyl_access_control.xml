<?xml version="1.0"?>
<block>
  <name>access_control</name>
  <key>ephyl_access_control</key>
  <category>[ephyl]</category>
  <import>import ephyl</import>
  <make>ephyl.access_control($bs_slots,$Control,$save_log,$ID,$log)</make>

<param>
    <name>BS Slots</name>
    <key>bs_slots</key>
    <value>[0]</value>
    <type>raw</type>
  </param>
  <param>
    <name>Access Control</name>
    <key>Control</key>
    <value>"basic"</value>
    <type>raw</type>
  </param>
  <param>
    <name>Save Log</name>
    <key>save_log</key>
    <value>False</value>
    <type>bool</type>
  </param>

  <param>
    <name>Sensor ID</name>
    <key>ID</key>
    <value>"Z"</value>
    <type>raw</type>
  </param>

  <param>
    <name>Generate debug/log file</name>
    <key>log</key>
    <value>False</value>
    <type>bool</type>
  </param>

  <sink>
    <name>DL</name>
    <type>message</type>
  </sink>
  <sink>
    <name>busy</name>
    <type>message</type>
  </sink>
  <sink>
    <name>inst</name>
    <type>message</type>
  </sink>

  <source>
    <name>PER</name>
    <type>message</type>
  </source>
  <source>
    <name>Data</name>
    <type>message</type>
  </source>
  <source>
    <name>Array</name>
    <type>message</type>
  </source>

</block>
